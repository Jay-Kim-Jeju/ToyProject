<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="toy.admin.member.service.impl.AdmMemberMapper">

    <!-- 간단 회원 목록 -->
    <select id="selectMemberList"
            parameterType="toy.com.vo.MemberVO"
            resultType="toy.com.vo.MemberVO">

        SELECT
            username,
            email
        FROM
            member
        ORDER BY
            username

    </select>

</mapper>
