<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="toy.admin.test.dao.AdmTestDAO">

    <!-- 간단 회원 목록 -->
    <select id="selectTestList"
            parameterType="toy.com.vo.TestVO"
            resultType="toy.com.vo.TestVO">

        SELECT
            username,
            email
        FROM
            test
        ORDER BY
            username

    </select>

</mapper>
